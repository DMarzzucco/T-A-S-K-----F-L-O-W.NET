generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum ROLES {
  BASIC
  ADMIN
}

model User {
  id           Int      @id @default(autoincrement())
  username     String   @unique @db.VarChar(255)
  password     String
  roles        ROLES
  refreshToken String?
  create_time  DateTime @default(now())
  update_time  DateTime @default(now()) @updatedAt
}
